
public class Star {
 public static void main(String[] args) {
	 for (int i=0;i<5;i++) {
		 for(int j=0;j<=i+1;j++) {
		 System.out.print("* ");
		 }
		 System.out.println();
	 }
 }
}
public class EBH204{
static boolean m1(String s, boolean b) {
	 System.out.print(s=(b?"T":"F"));
	 return b;
  }
public static void main(String[] args) {
	m1("A",m1("B",false)||("C",true)||m1("D",false));
}
}
